!		<Key>BackSpace: string(0x7F)
!		XTerm*ttyModes: erase ^?


!urxvt.font: -*-dejavu sans mono-medium-r-*-*-13-*-*-*-*-*-iso10646-*
urxvt.font: -*-terminus-medium-*-*-*-*-140-*-*-*-*-*
!URxvt*font: xft:DejaVu Sans Mono:pixelsize=10
urxvt.background: black
urxvt.foreground: white
urxvt*urllauncher: conkeror
urxvt*matcher.button:        1
urxvt*saveLines:              8192
URxvt.perl-ext-common: default,matcher
URxvt.urlLauncher: conkeror
URxvt.colorUL: #86a2be
urxvt.keysym.M-u: perl:mark-yank-urls:activate_mark_mode
urxvt*scrollBar: false
urxvt*urgentOnBell:true

conapp.font: -*-terminus-medium-*-*-*-*-140-*-*-*-*-*
